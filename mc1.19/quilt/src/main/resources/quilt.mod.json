{
  "schema_version": 1,
  "mixin": [
    "invmove-quilt.mixins.json",
    "invmove-fabric-like.mixins.json",
    "invmove.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "invmove",
    "version": "${version}",
    "metadata": {
      "name": "InvMove",
      "description": "Lets you move while in inventories.",
      "contributors": {
        "PieKing1215": "Owner"
      },
      "contact": {
        "homepage": "https://github.com/PieKing1215/InvMove",
        "sources": "https://github.com/PieKing1215/InvMove",
        "issues": "https://github.com/PieKing1215/InvMove/issues"
      },
      "icon": "icon.png",
      "license": "LGPL-3.0-only"
    },
    "minecraft": {
      "environment": "client"
    },
    "entrypoints": {
      "client_init": "me.pieking1215.invmove.quilt.InvMoveQuilt",
      "modmenu": "me.pieking1215.invmove.fabric_like.InvMoveModMenuIntegration"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19"
      },
      {
        "id": "cloth-config",
        "version": ">=7"
      }
    ],
    "breaks": [
      {
        "id": "invmove_compat",
        "version": "<=0.1.0"
      }
    ]
  }
}